{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build (MSVC)",
      "type": "process",
      "command": "cmd.exe",
      "args": [
        "/d",
        "/c",
        "${workspaceFolder}\\build.bat"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$msCompile"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}


